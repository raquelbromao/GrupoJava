
public class TesteFor {
	
	public static void main(String[] args) {
		
		for (int i = 0; i < 10; i++) {
			System.out.println("Olá!");
		}
		
		
		for (int i = 0; i < 100; i++) {
			 if (i % 19 == 0) {
			    System.out.println("Achei um número divisível por 19 entre x e y = " + i);
			    //break;
			  }
		}
		
		for (int i = 0; i < 100; i++) {
			  if (i > 50 && i < 60) {
			    continue;
			  }
			  System.out.println(i);
			}
		
	}
}
